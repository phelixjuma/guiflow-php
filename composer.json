{
  "name": "phelixjuma/guiflow",
  "description": "GUIFlow is a no-code workflow builder",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "wyndow/fuzzywuzzy": "^0.6.0",
    "erdalceylan/array-join": "^2.1",
    "ext-openssl": "*",
    "jumaphelix/php-dag": "^2.0",
    "opis/json-schema": "^2.3"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "human-json": "generate-schema-doc ../src/SchemaDefinitions/config-schema.v3.json --config template_name=js_offline"
  },
  "autoload": {
    "psr-4": {
      "PhelixJuma\\GUIFlow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhelixJuma\\GUIFlow\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  }
}
